{
    "name": "rev/laravel-amqp",
    "description": "A Laravel wrapper for RabbitMQ/AMQP with automatic reconnection and graceful shutdown",
    "keywords": [
        "laravel",
        "rabbitmq",
        "amqp",
        "queue",
        "messaging",
        "rpc"
    ],
    "type": "library",
    "license": "MIT-0",
    "authors": [],
    "require": {
        "php": "^8.1|^8.2|^8.3|^8.4",
        "illuminate/support": "^10.0|^11.0",
        "laravel/framework": "^11.0|^12.0",
        "php-amqplib/php-amqplib": "^3.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0",
        "mockery/mockery": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "REV\\LaravelAmqp\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "REV\\LaravelAmqp\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "REV\\LaravelAmqp\\AmqpServiceProvider"
            ],
            "aliases": {
                "Amqp": "REV\\LaravelAmqp\\Facades\\Amqp"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    }
}